@model IEnumerable<HMS_Project.Models.TBLUser>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/_LayoutPage1.cshtml";
}
<br />
<h4>USER</h4>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.User_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User_Phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User_Gender)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User_Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User_Password)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.User_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User_Phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User_Gender)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User_Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User_Password)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
